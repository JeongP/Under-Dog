<p id="notice"><%= notice %></p>

<h1>Bidings</h1>

<table>
  <thead>
    <tr>
      <th>Price</th>
      <th>Success bid</th>
      <th>Post</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bidings.each do |biding| %>
      <tr>
        <td><%= biding.price %></td>
        <td><%= biding.success_bid %></td>
        <td><%= biding.post_id %></td>
        <td><%= biding.user_id %></td>
        <td><%= link_to 'Show', biding %></td>
        <td><%= link_to 'Edit', edit_biding_path(biding) %></td>
        <td><%= link_to 'Destroy', biding, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Biding', new_biding_path %>
